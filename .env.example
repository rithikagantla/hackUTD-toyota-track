# Toyota Nexus - Environment Variables Example
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=8080
CORS_ORIGIN=http://localhost:5173

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:8080

# Gemini API (Required)
# Get your API key from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Auth0 Configuration (Optional - only needed if REQUIRE_AUTH_FOR_CHAT=true)
REQUIRE_AUTH_FOR_CHAT=false
AUTH0_ISSUER=
AUTH0_AUDIENCE=https://api.toyota-nexus
VITE_AUTH0_DOMAIN=
VITE_AUTH0_CLIENT_ID=
VITE_AUTH0_AUDIENCE=https://api.toyota-nexus

# Note: Never commit your .env file with real API keys to version control!
